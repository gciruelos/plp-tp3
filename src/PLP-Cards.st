Object subclass: #Card	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!Card subclass: #GoalCard	instanceVariableNames: 'set'	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!!GoalCard methodsFor: 'as yet unclassified'!= arg1	^ arg1 class == self class and: [ self content = arg1 content ]! !!GoalCard methodsFor: 'as yet unclassified'!content	^ set! !!GoalCard methodsFor: 'as yet unclassified'!hash	^ set hash! !!GoalCard methodsFor: 'as yet unclassified'!includes: arg1	^ set includes: arg1! !!GoalCard methodsFor: 'as yet unclassified'!isMetBy: arg1	^ arg1 includesAllOf: set! !!GoalCard methodsFor: 'as yet unclassified'!play: arg1 withGame: arg2	^ arg2 setGoal: self! !!GoalCard methodsFor: 'as yet unclassified'!printString	^ (set collect: [ :arg1 | arg1 printString ]) printString! !!GoalCard methodsFor: 'as yet unclassified'!setSet: arg1	set := arg1! !!GoalCard methodsFor: 'as yet unclassified'!~ arg1	^ (self = arg1) not! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!GoalCard class	instanceVariableNames: ''!!GoalCard class methodsFor: 'as yet unclassified'!new: arg1	^ super new setSet: arg1! !Card subclass: #TreasureCard	instanceVariableNames: 'text'	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!!TreasureCard methodsFor: 'as yet unclassified'!= arg1	^ arg1 class == self class and: [ self getText = arg1 getText ]! !!TreasureCard methodsFor: 'as yet unclassified'!getText	^ text! !!TreasureCard methodsFor: 'as yet unclassified'!hash	^ self getText hash! !!TreasureCard methodsFor: 'as yet unclassified'!play: arg1 withGame: arg2	arg1 add: self! !!TreasureCard methodsFor: 'as yet unclassified'!printString	^ text printString! !!TreasureCard methodsFor: 'as yet unclassified'!setText: arg1	text := arg1! !!TreasureCard methodsFor: 'as yet unclassified'!~= arg1	^ (self = arg1) not! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!TreasureCard class	instanceVariableNames: ''!!TreasureCard class methodsFor: 'as yet unclassified'!new: arg1	^ super new setText: arg1! !