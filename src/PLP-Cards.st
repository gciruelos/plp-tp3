Object subclass: #Card	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!Card subclass: #GoalCard	instanceVariableNames: 'set'	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:25'!= anotherGoalCard	^ anotherGoalCard class = self class and: [ self content = anotherGoalCard content ]! !!GoalCard methodsFor: 'as yet unclassified'!content	^ set! !!GoalCard methodsFor: 'as yet unclassified'!hash	^ set hash! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:25'!includes: aTreasureCard	^ set includes: aTreasureCard! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:24'!init: aSet	set := aSet.! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:24'!isMetBy: aTable	^ aTable includesAllOf: set! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:24'!play: aTable withGame: aGame	^aGame setGoal: self! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:24'!printString	^ (set collect: [ :c | c printString ]) printString! !!GoalCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:26'!~ anotherGoalCard	^ (self = anotherGoalCard) not! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!GoalCard class	instanceVariableNames: ''!!GoalCard class methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:23'!new: aSet	^ super new init: aSet! !Card subclass: #TreasureCard	instanceVariableNames: 'text'	classVariableNames: ''	poolDictionaries: ''	category: 'PLP-Cards'!!TreasureCard methodsFor: 'as yet unclassified' stamp: 'GT 6/22/2017 01:26'!= anotherGoalCard	^ anotherGoalCard class = self class and: [ self getText = anotherGoalCard getText ]! !!TreasureCard methodsFor: 'as yet unclassified'!getText	^ text! !!TreasureCard methodsFor: 'as yet unclassified'!hash	^ self getText hash! !!